{
  "name": "my-wasm-sdk",
  "version": "0.1.0",
  "description": "A Go Wasm SDK generated from an OpenAPI specification.",
  "main": "dist/lib.wasm",
  "types": "dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./build.sh",
    "gen_ts":"npx openapi --input ./openapi.yaml --output ./build --client fetch",
    "gen_swg":"npx swagger-typescript-api generate --path ./openapi.yaml --another-array-type --clean-output --output=./src --sort-types --sort-routes --name=Api.ts"

  },
  "keywords": [
    "wasm",
    "go",
    "openapi",
    "sdk"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "openapi-generator": "^0.1.39"
  },
  "devDependencies": {
    "openapi-typescript": "^7.9.1",
    "openapi-typescript-codegen": "^0.29.0",
    "swagger-typescript-api": "^13.2.8"
  }
}
